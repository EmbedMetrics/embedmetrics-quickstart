# EmbedMetrics React Demo Environment Variables
# Copy this file to .env and update with your actual values

# Your app ID from the EmbedMetrics Admin Console (same as server-token-minting)
REACT_APP_APP_ID=your-app-id-from-admin-console

# EmbedMetrics Platform API base URL (same as server-token-minting)
REACT_APP_PLATFORM_API_URL=http://localhost:8080

# Token server URL (where your backend runs)
# This should match your server-token-minting server
REACT_APP_TOKEN_SERVER_URL=http://localhost:3001/demo/token
