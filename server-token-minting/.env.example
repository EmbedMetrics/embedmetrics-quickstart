# EmbedMetrics Demo Server Environment Variables
# Copy this file to .env and update with your actual values

# EmbedMetrics Platform API base URL
PLATFORM_API_URL=https://api.embedmetrics.com

# Your app ID from the EmbedMetrics Admin Console
APP_ID=your-app-id-from-admin-console

# Client secret for your app (starts with emsk_)
APP_CLIENT_SECRET=emsk_live_XXXXXXXXXX_YYYYYYYYYYYYYYYYYYYY

# Comma-separated list of allowed CORS origins (optional)
CORS_ORIGINS=http://localhost:3000,http://localhost:5173,http://localhost:5174

# Local server port (optional, defaults to 3001)
PORT=3001
